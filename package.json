    "typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-reply-from.git"
  },
  "keywords": [
    "fastify",
    "http",
    "forward",
    "proxy"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-reply-from/issues"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "homepage": "https://github.com/fastify/fastify-reply-from#readme",
  "devDependencies": {
    "@sinonjs/fake-timers": "^7.0.0",
    "@types/node": "^14.14.2",
    "@types/tap": "^14.10.1",
    "fastify": "^3.7.0",
    "fastify-multipart": "^4.0.0",
    "form-data": "^3.0.0",
    "got": "^11.8.0",
    "h2url": "^0.2.0",
    "msgpack5": "^5.0.0",
    "nock": "^13.0.4",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.3",
    "simple-get": "^4.0.0",
    "snazzy": "^9.0.0",
    "split2": "^3.2.2",
