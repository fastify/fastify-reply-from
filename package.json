    "end-of-stream": "^1.4.1",
    "fastify-plugin": "^3.0.0",
    "http-errors": "^1.8.0",
    "pump": "^3.0.0",
    "semver": "^7.2.1",
    "tiny-lru": "^7.0.0",
    "undici": "^2.1.0"
