{
  "name": "@fastify/reply-from",
  "version": "7.0.0",
  "description": "forward your HTTP request to another server, for fastify",
  "main": "index.js",
  "types": "index.d.ts",
  "scripts": {
    "coverage": "tap -j8 test/*.js --cov --coverage-report=html",
    "lint:fix": "standard --fix",
    "test": "standard | snazzy && tap test/*.js && npm run typescript",
    "test:ci": "standard | snazzy && tap test/*.js --coverage-report=lcovonly && npm run typescript",
    "typescript": "tsd"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/fastify/fastify-reply-from.git"
  },
  "keywords": [
    "fastify",
    "http",
    "forward",
    "proxy"
  ],
  "author": "Matteo Collina <hello@matteocollina.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fastify/fastify-reply-from/issues"
  },
  "engines": {
    "node": ">=12.18"
  },
  "homepage": "https://github.com/fastify/fastify-reply-from#readme",
  "devDependencies": {
    "@fastify/formbody": "^6.0.0",
    "@fastify/multipart": "^6.0.0",
    "@sinonjs/fake-timers": "^9.0.0",
    "@types/node": "^17.0.0",
    "@types/tap": "^15.0.3",
    "fastify": "^3.17.0",
    "form-data": "^4.0.0",
    "got": "^11.8.2",
    "h2url": "^0.2.0",
    "msgpack5": "^6.0.0",
    "nock": "^13.1.0",
    "pre-commit": "^1.2.2",
    "proxyquire": "^2.1.3",
    "simple-get": "^4.0.0",
    "snazzy": "^9.0.0",
    "split2": "^4.0.0",
    "standard": "^17.0.0",
    "tap": "^16.0.0",
    "tsd": "^0.20.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "end-of-stream": "^1.4.4",
    "fastify-plugin": "^3.0.0",
    "http-errors": "^2.0.0",
    "pump": "^3.0.0",
    "semver": "^7.3.5",
    "tiny-lru": "^8.0.1",
    "undici": "^5.0.0"
  },
  "tsd": {
    "directory": "test"
